{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "session",
          "foreground": "#45F1C2",
          "style": "plain",
          "template": "{{ .UserName }}@{{ .HostName }}"
        },
        {
          "type": "path",
          "foreground": "#0CA0D8",
          "properties": {
            "folder_separator_icon": "/",
            "style": "full"
          },
          "style": "plain",
          "template": " {{ .Path }} "
        },
        {
          "foreground": "#14A5AE",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_stash_count": true,
            "fetch_upstream_icon": true
          },
          "type": "git",
          "style": "plain",
          "template": "{{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} "
        },
        {
          "type": "kubectl",
          "style": "plain",
          "foreground": "#0CA0D8",
          "template": "{{if .Context }}<#b38600>{{.Context}}</>::{{if .Namespace}}<#d33682>{{.Namespace}}</>{{else}}default{{end}} {{end}}"
        },
        {
          "type": "text",
          "style": "plain",
          "template": "\n"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#14A5AE",
          "template": "{{ if eq .Env.FISH__BIND_MODE \"default\" }}â„• {{ else if eq .Env.FISH__BIND_MODE \"insert\" }}> {{ else if eq .Env.FISH__BIND_MODE \"replace_one\" }}<green>[R]</>{{ else if eq .Env.FISH__BIND_MODE \"visual\"}}v {{ else }}<red>[?]</>{{ end }}"
        }
      ]
    }
  ],
  "version": 2
}
